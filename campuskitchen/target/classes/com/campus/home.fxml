<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controllers.SecondaryController">
    <children>
        <Label text="Secondary View" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="btnRecipes" mnemonicParsing="false" text="Recipes" />
            <Button fx:id="btnMealPlan" mnemonicParsing="false" text="Meal Plans" />
            <Button fx:id="btnBrowze" mnemonicParsing="false" text="Browze" />
            <Button fx:id="btnhome" mnemonicParsing="false" text="Home" />
        </buttons>
      </ButtonBar>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
